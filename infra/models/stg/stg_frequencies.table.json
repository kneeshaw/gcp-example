{
  "schema": {
    "fields": [
      {
        "name": "created_at",
        "type": "TIMESTAMP",
        "mode": "NULLABLE",
        "description": "Row creation timestamp (warehouse)"
      },
      {
        "name": "updated_at",
        "type": "TIMESTAMP",
        "mode": "NULLABLE",
        "description": "Last update timestamp (warehouse)"
      },
      {
        "name": "trip_id",
        "type": "STRING",
        "mode": "REQUIRED",
        "description": "Trip identifier for this frequency"
      },
      {
        "name": "start_time",
        "type": "STRING",
        "mode": "REQUIRED",
        "description": "Start time for this frequency period"
      },
      {
        "name": "end_time",
        "type": "STRING",
        "mode": "REQUIRED",
        "description": "End time for this frequency period"
      },
      {
        "name": "headway_secs",
        "type": "INT64",
        "mode": "REQUIRED",
        "description": "Time between departures in seconds"
      },
      {
        "name": "exact_times",
        "type": "INT64",
        "mode": "NULLABLE",
        "description": "Indicates if frequencies are exact (0=frequency-based, 1=exact schedule)"
      },
      {
        "name": "feed_hash",
        "type": "STRING",
        "mode": "REQUIRED",
        "description": "Hash of the GTFS feed for data lineage"
      }
    ]
  },
  "description": "Trip frequencies and headways from GTFS schedule feeds",
  "clustering": {
    "fields": [
      "feed_hash",
      "trip_id"
    ]
  }
}
