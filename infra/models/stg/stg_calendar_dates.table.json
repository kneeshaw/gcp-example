{
  "schema": {
    "fields": [
      {
        "name": "created_at",
        "type": "TIMESTAMP",
        "mode": "NULLABLE",
        "description": "Row creation timestamp (warehouse)"
      },
      {
        "name": "updated_at",
        "type": "TIMESTAMP",
        "mode": "NULLABLE",
        "description": "Last update timestamp (warehouse)"
      },
      {
        "name": "service_id",
        "type": "STRING",
        "mode": "REQUIRED",
        "description": "Service identifier this exception applies to"
      },
      {
        "name": "date",
        "type": "INT64",
        "mode": "REQUIRED",
        "description": "Date of the service exception in YYYYMMDD format"
      },
      {
        "name": "exception_type",
        "type": "INT64",
        "mode": "REQUIRED",
        "description": "Type of exception (1=added, 2=removed)"
      },
      {
        "name": "feed_hash",
        "type": "STRING",
        "mode": "REQUIRED",
        "description": "Hash of the GTFS feed for data lineage"
      }
    ]
  },
  "description": "Service exception dates from GTFS schedule feeds",
  "clustering": {
    "fields": [
      "feed_hash",
      "service_id",
      "date"
    ]
  }
}
